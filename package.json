{
  "name": "simple-react-full-stack",
  "version": "1.0.0",
  "description": "Boilerplate to build a full stack web application using React, Node.js, Express and Webpack.",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode development --config webpack.config.js --progress",
    "ship": "webpack --mode production --config webpack.config.production --progress",
    "start": "npm run build && node src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Sandeep Raveesh",
  "license": "ISC",
  "dependencies": {
    "antd": "^3.24.3",
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "date-fns": "^2.8.1",
    "debug": "^4.1.1",
    "ejs": "^3.0.1",
    "excel4node": "^1.7.2",
    "express": "^4.16.3",
    "haversine-distance": "^1.2.1",
    "immutability-helper": "^3.1.1",
    "immutable": "^4.0.0-rc.12",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.12",
    "multer": "^1.4.2",
    "prettify-xml": "^1.2.0",
    "query-string": "^6.8.3",
    "random-location": "^1.1.3",
    "react": "^16.5.2",
    "react-datepicker": "^2.10.0",
    "react-dom": "^16.5.2",
    "react-redux": "^7.1.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.4",
    "xml-formatter": "^2.4.0"
